    <!-- 查询学生，根据id -->  
    <select id="getMaterialMaster" parameterType="Map" resultType="com.chlitina.o2o.entity.impl.MaterialMasterImpl" resultMap="materialMasterMap">  
        <![CDATA[  
        	select * from  (
        		select rownum as rowno, t.* from zsdss_material_master t where rownum<= ${rowend} ) a 
        	where a.rowno > ${rowstart}
        ]]>    
    </select>  

    <select id="getMaterialMasterCount" resultType="int">  
        <![CDATA[  
        	select count(*) from zsdss_material_master where  LOEVM <> 'X' 
        ]]>    
    </select>
    
    <select id="getCustMaster" parameterType="Map" resultType="com.chlitina.o2o.entity.impl.CustMasterImpl" resultMap="custMasterMap">  
        <![CDATA[  
			select * from  (
				select rownum as rowno, t.* from ZSDSS_CUST_MASTER t where  rownum < ${rowend})  a 
			where a.rowno >= ${rowstart}
        ]]>    
    </select>
    
    <select id="getStoreMonBonus" parameterType="Map" resultType="com.chlitina.o2o.entity.impl.StoreMonBonusImpl" resultMap="storeMonBonusMap">  
        <![CDATA[  
			select * from  (
    			select rownum as rowno, t.* from T_DX_MON_BONUS t where BONUS_YEAR=#{year} and BONUS_MON=#{mon} and rownum<= ${rowend})  a 
    		where a.rowno > ${rowstart}
        ]]>    
    </select>
    
    <select id="getStoreMonBonusSec" parameterType="Map" resultType="com.chlitina.o2o.entity.impl.StoreMonBonusSecImpl" resultMap="storeMonBonusSecMap">  
        <![CDATA[  
        	select * from  (
        		select rownum as rowno, t.* from T_DX_MON_BONUS_2 t where PERIOD = #{period} and rownum<= ${rowend})  a 
        	where a.rowno> ${rowstart}
        ]]>    
    </select>
    
    <select id="getStoreBonusDefine" parameterType="Map" resultType="com.chlitina.o2o.entity.impl.StoreBonusDefineImpl" resultMap="storeBonusDefineMap">  
        <![CDATA[  
        	select * from  (
    			select rownum as rowno, t.* from T_O2O_BONUS_DEFINE t where BONUS_YEAR=${year} and BONUS_MONTH=${mon} and rownum<= ${rowend}) a 
    		where a.rowno > ${rowstart}
        ]]>    
    </select>
    
    <select id="getOrdersByDay" parameterType="Map" resultType="com.chlitina.o2o.entity.impl.OrderHeaderImpl" resultMap="ordersMap">  
        <![CDATA[  
        	select * from (
        		select * from  (
                	select rownum as rowno, t.* from zsdss_order_header t 
                	where CDATE = ${cdate} and vkorg<> '1051'  and rownum<= ${rowend}) a 
        		where a.rowno > ${rowstart}) temp,  zsdss_order_item i 
			where temp.vbeln=i.vbeln
        ]]>    
    </select>
    
    <select id="getOrdersByMon" parameterType="Map" resultType="com.chlitina.o2o.entity.impl.OrderHeaderMonImpl" resultMap="monOrdersMap">
        <![CDATA[  
        	select * from (
        		select * from  (
                	select rownum as rowno, t.* from zsdss_order_header_mon t 
                	where rownum<= ${rowend}) a where a.rowno > ${rowstart}) temp,  zsdss_order_item_mon i 
			where temp.vbeln=i.vbeln
        ]]>    
    </select>
    
    <select id="getOrderHeadersByDay" parameterType="Map" resultType="com.chlitina.o2o.entity.impl.OrderHeaderImpl">  
        <![CDATA[  
        	select * from  (
				select rownum as rowno, t.* from zsdss_order_header t where CDATE = #{cdate} and vkorg<> '1051' and rownum<= ${rowend}) a 
