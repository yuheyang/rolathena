        ]]>    
    </select>
    
    <select id="getOrderItemsById" parameterType="String" resultType="com.chlitina.o2o.entity.impl.OrderItemImpl">  
        <![CDATA[  
        	select rownum as rowno, t.* from zsdss_order_item t where vbeln in ( ${vbelnstring} ) order by vbeln
        ]]>    
    </select>
    
    <select id="getProductReportByDay" parameterType="String" resultType="com.chlitina.o2o.entity.impl.ProductReportImpl">  
        <![CDATA[  
        	select * from  (
				select rownum as rowno, t.* from zva05 t where audat = #{querydate} and rownum<= ${rowend}) a 
