    <select id="getAreaBonusByDay" parameterType="Map" resultType="com.chlitina.o2o.entity.impl.AreaBonusImpl">  
        <![CDATA[  
        	select * from  (
				select rownum as rowno, t.* from ZSSDR002 t where DATUM = #{querydate} and rownum<= ${rowend}) a 
